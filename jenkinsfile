node{
stage('scm'){ 
        git 'https://github.com/chrishantha/sample-java-programs.git'
            }
       
    
    stage('Build maven') {
         sh 'mvn install'
        }
      
    stage('sonarqube') {
        withSonarQubeEnv('sonar') {
        sh 'mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.7.0.1746:sonar'
        } 
        }  
}
